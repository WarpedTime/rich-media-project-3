<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>Slimez[app]</title>
  <script crossorigin src="https://unpkg.com/react@15/dist/react.min.js"></script>
  <script crossorigin src="https://unpkg.com/react-dom@15/dist/react-dom.min.js"></script>
  <script crossorigin src="https://unpkg.com/prop-types/prop-types.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/remarkable/1.6.2/remarkable.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <link rel="shortcut icon" href="/assets/img/favicon.png"/>
  <link rel="stylesheet" type="text/css" href="/assets/style.css"/>
</head>
<body>
  
  <nav><a href="/login"><img id="logo" src="/assets/img/favicon.png" alt="logo"/></a>
    <div class="navlink"><b>Gacha Slimez:</b></div>
    
    <div id="docButton" class="navlink"><a>[global things]</a></div>
    <div id="docButton" class="navlink"><a>[updates/news]</a></div>
    
    <div id="aboutButton" class="navlink nav-right"><a>[user][user-icon]</a></div>
  </nav>
  <nav id="sub-nav">
    <div id="aboutButton" class="navlink"><a>About</a></div>
    <div id="slimeListButton" class="navlink"><a>Slime List</a></div>
    <div id="docButton" class="navlink"><a>Documentation</a></div>  
    <div class="navlink nav-right" ><a href="/logout">Log out</a></div>
  </nav>
      
    <section id="makeSlime">
    </section>
    
    <div id="slimeViewContainer">
      <section id="slimes" class="view-pannel left">
      </section>
      <section id="slimeInfoPannel" class="detail-pannel">
        <h3>- Details -</h3>
        <img src='/assets/img/17.png' alt='slime' />
        <h3>Name: Goggles</h3>
        <h3>id: 17</h3>  
      </section>
    </div>
  
  
  <br style="clear:both" />
    
  <div id="slimeMessage"><img id="speech" src="/assets/img/speech.png" alt="speech box"/>
    <h3><span id="errorMessage"></span></h3><img id="slime" src="assets/img/player - Copy.png" alt="slime"/>
  </div>
  
  <div id="footer">
    <p class="header">- Copyright 2017-2018 Danielle Carmi - </p>
    <p>All rights reserved</p>
  </div>

  <div id="audio" class="hidden">
    <audio id="bgAudio" controls loop></audio>
    <audio id="effectAudio" controls></audio>
  </div>  
    
  <script src="/assets/bundle.js"></script>
</body>
</html>